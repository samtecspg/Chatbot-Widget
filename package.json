{
  "name": "articulate-chatbot-widget",
  "version": "1.0.0",
  "description": "Chatbot widget for Articulate based on JiteshGaikwad Chatbot-Widget",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "dev": "cp src/style.css dev/ && webpack-dev-server --config ./dev/webpack.config.js",
    "lint": "eslint src || exit 0",
    "lint-fix": "eslint src --fix || exit 0",
    "prettier": "prettier --config .prettierrc --write src/**/**/*.js",
    "build": "cross-env babel src --out-dir lib",
    "copy": "cp src/style.css lib/",
    "prepublish": "npm run clean && npm run build && npm run copy",
    "dist": "webpack --config ./webpack.config.prod.js"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samtecspg/articulate-chatbot-widget"
  },
  "keywords": [
    "boilerplate",
    "react",
    "npm"
  ],
  "author": "Samtec SPG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samtecspg/articulate-chatbot-widget/issues"
  },
  "homepage": "https://github.com/samtecspg/articulate-chatbot-widget",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^5.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.13.7",
    "rimraf": "^2.5.4",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.35.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.1",
    "@material-ui/icons": "^4.5.1",
    "classnames": "^2.2.6",
    "guid": "^0.0.12",
    "materialize-css": "^1.0.0",
    "nes": "^10.0.2",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
